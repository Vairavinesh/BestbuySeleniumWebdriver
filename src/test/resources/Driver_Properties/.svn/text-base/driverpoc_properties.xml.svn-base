<?xml version='1.0' encoding='utf-8' ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
	<comment>main Driver properties
	To validate all solr and magellan files set stopFile to all.
	Note: All Delta files are validated by default.</comment>
	<entry key="dataResourcePath">src/main/resources/Testcases_Functions_DataResource_XMLS/dataresources/</entry>
	<entry key="logsFolder">src/main/resources/logs/</entry>
	<entry key="dataResourcesPath">src/main/resources/Testcases_Functions_DataResource_XMLS/dataresources/</entry>
	<entry key="masterXML">src/main/resources/Testcases_Functions_DataResource_XMLS/Runners/mdot/MasterXMLSample.xml</entry>
	<entry key="masterXmlPath">src/main/resources/Testcases_Functions_DataResource_XMLS/Runners/mdot/</entry>
	<entry key="testcaseXmlPath">src/main/resources/Testcases_Functions_DataResource_XMLS/Testcases/</entry>
	<entry key="xPathDefinitionFilePath">xPath_Definitions/xpaths.xml</entry>
	<entry key="environmentConfig">Environment_Configuration/EnvironmentDefinitions.xml</entry>
	<entry key="firefoxPathMac">/Applications/Firefox.app/Contents/MacOS/firefox-bin</entry>
	<entry key="logsFolder">src/main/resources/logs/</entry>
	<entry key="functionBasePath">src/main/resources/Testcases_Functions_DataResource_XMLS/Functions/</entry>
	<entry key="ajaxwaittimeout">30</entry>
	<entry key="testcaseFilesStartWith">test</entry>
	<entry key="masterInfo">//*/master</entry>
	<entry key="testInfoXpath">//*/test</entry>
	<entry key="testcaseBaseXpath">//*/testcase[(contains(@versions, '~versionVar~') or contains(@versions, '*')]/.</entry>
	<entry key="testToRunXpath">//*/testcase[@run='true']/.</entry>
	<entry key="groupTestcaseXpath">//*/test[(@application='~application~' and ((@first_version&lt;='~version~' and @last_version&gt;='~version~') or (@first_version&lt;='~version~' and @last_version='*')))~attrib~]</entry>
	<entry key="groupAttrib">contains(@groupIds,'~groupid~')</entry>
	<entry key="functionGroupAttrib">contains(@functional_group,'~functional_group~')</entry>
	<entry key="subFunctionalGroupAttrib">contains(@functional_subgroup, '~subfunctional_group~')</entry>
	<entry key="testcaseStepXpath">//*/step</entry>
	<entry key="attribContains">contains(@~nodeIdVar~, '" + ~attribVar~ + "')</entry>
	<entry key="dataResourceXpath">//*/data/*</entry>
	<entry key="dateFormat">[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z|[0-9]{4}-[0-9]{2}-[0-9]{2}|[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}\.[0-9]{3}-[0-9]{2}:[0-9]{2}</entry>
	<entry key="defaultResolution_firefox">1024x768</entry>
</properties>
